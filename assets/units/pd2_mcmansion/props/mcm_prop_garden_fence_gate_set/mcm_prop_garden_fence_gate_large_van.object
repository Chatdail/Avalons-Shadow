<dynamic_object>
	<diesel materials="units/pd2_mcmansion/props/mcm_prop_garden_fence_gate_set/mcm_prop_garden_fence_gate_set" orientation_object="rp_mcm_prop_garden_fence_gate_large" />
	<sequence_manager file="units/pd2_mcmansion/props/mcm_prop_garden_fence_gate_set/mcm_prop_garden_fence_gate_large_van" />
	<animations>
		<animation_group name="anim" enabled="false">
			<object name="anim_r" />
			<object name="anim_l" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="rp_mcm_prop_garden_fence_gate_large" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
			<object name="c_box_02" collision_type="box" padding="-2.5" />
			<object name="c_capsule_01" collision_type="capsule" padding="-2.5" />
			<object name="c_capsule_02" collision_type="capsule" padding="-2.5" />
		</body>
		<body name="body_r" enabled="true" template="animated">
			<object name="anim_r" />
			<object name="c_meshmopp_gate_right" collision_type="mesh_mopp" />
		</body>
		<body name="body_l" enabled="true" template="animated">
			<object name="anim_l" />
			<object name="c_meshmopp_gate_left" collision_type="mesh_mopp" />
		</body>
    </bodies>

	<decal_surfaces default_material="wood">
		<decal_mesh name="dm_plaster" enabled="true" material="plaster" />
		<decal_mesh name="dm_wood_left" enabled="true" material="wood" />
		<decal_mesh name="dm_wood_right" enabled="true" material="wood" />
	</decal_surfaces>

	<graphics>
		<object name="g_gate_left" enabled="true" />
		<object name="g_gate_right" enabled="true" />
		<object name="s_gate_left" enabled="true" shadow_caster="true" />
		<object name="s_gate_right" enabled="true" shadow_caster="true" />
		<object name="g_pillars" enabled="true" />
		<object name="s_pillars" enabled="true" shadow_caster="true" />
	</graphics>

</dynamic_object>

