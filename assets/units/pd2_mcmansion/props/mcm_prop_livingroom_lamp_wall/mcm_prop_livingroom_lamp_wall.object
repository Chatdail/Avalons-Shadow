<dynamic_object>
	<diesel materials="units/pd2_mcmansion/props/mcm_prop_livingroom_lamp_wall/mcm_prop_livingroom_lamp_wall" orientation_object="rp_mcm_prop_livingroom_lamp_wall" />
	<sequence_manager file="units/pd2_mcmansion/props/mcm_prop_livingroom_lamp_wall/mcm_prop_livingroom_lamp_wall" />
	<bodies>
		<body name="body_static" enabled="true" template="static">
			<object name="rp_mcm_prop_livingroom_lamp_wall" />
			<object name="c_capsule" collision_type="capsule" padding="-2.5" />
		</body>
	</bodies>
	<graphics>
		<graphic_group name="grp_lit" enabled="true" >
			<lod_object name="grp_lod_g_lit">
				<object name="g_lit_lod0" enabled="true" max_distance="4000" />
				<object name="g_lit_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
		<graphic_group name="grp_unlit" enabled="false" >
			<lod_object name="grp_lod_g_unlit">
				<object name="g_unlit_lod0" enabled="true" max_distance="4000" />
				<object name="g_unlit_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
		<graphic_group name="grp_dmg" enabled="false" >
			<lod_object name="grp_lod_g_dmg">
				<object name="g_dmg_lod0" enabled="true" max_distance="4000" />
				<object name="g_dmg_lod1" enabled="true" />
			</lod_object>
		</graphic_group>
	</graphics>

	<lights>
		<light name="li_lamp" enabled="true" editable="true" type="omni|specular" multiplier="0.6" far_range="50" falloff="4" intensity="identity" />
	</lights>

</dynamic_object>
