		<object name="g_welding_frame_02" enabled="false" />
		<object name="g_welding_frame_03" enabled="false" />
		<object name="g_welding_frame_04" enabled="false" />


		<lod_object name="lod_g_door">
			<object name="g_door_lod0" enabled="true" max_distance="2500" />
			<object name="g_door_lod1" enabled="true" />
		</lod_object>
		<lod_object name="lod_g_frame">
			<object name="g_frame_lod0" enabled="true" max_distance="2500" />
			<object name="g_frame_lod1" enabled="true" />
