<?xml version="1.0"?>
<dynamic_object>
	<animation_def name="anims/units/helicopter/helicopter" />
	<diesel materials="units/pd2_dlc_jerry/vehicles/jry_vehicle_blackhawk_money_pile/jry_vehicle_blackhawk_money_pile" orientation_object="rp_helicopter" />
	<sequence_manager file="units/pd2_dlc_jerry/vehicles/jry_vehicle_blackhawk_money_pile/jry_vehicle_blackhawk_money_pile" />

  	<animations>
		<animation_group name="ag_door_left">
			<object name="a_door_left" />
		</animation_group>
		<animation_group name="ag_door_right">
			<object name="a_door_right" />
		</animation_group>
		<animation_group name="ag_main_rotor">
			<object name="a_main_rotor" />
			<object name="a_second_rotor" />
		</animation_group>
		<animation_group name="a_main_animation">
			<object name="a_body" />
		</animation_group>
		<animation_group name="money_pile_lift">
			<object name="joint1" />
			<object name="joint2" />
			<object name="joint3" />
			<object name="joint4" />
			<object name="joint5" />
			<object name="joint6" />
			<object name="joint_crate" />
		</animation_group>
	</animations>
	
	<bodies>
		<body name="editor_collision" enabled="true" template="editor">
			<object name="c_box" collision_type="box" />
		</body>
	</bodies> 
  <graphics>
	<graphic_group name="heli_gfx_grp" enabled="true" culling_object="co_helicopeter">
		<object name="g_interior" 					enabled="true" />
		<object name="g_door_right_nypd" 			enabled="true" />
		<object name="g_door_right_swat" 			enabled="false" />
		<object name="g_door_left_swat" 			enabled="false" />
		<object name="g_door_left_nypd" 			enabled="true" />
		<object name="g_main_rotor" 				enabled="true" />
		<object name="g_second_rotor" 				enabled="true" />
		<object name="g_glass" 						enabled="true" />
		<object name="g_body_nypd" 					enabled="true" />
		<object name="g_body_swat" 					enabled="false" />
		<object name="g_light_holder" 				enabled="false" /> 
		<object name="g_spotlight" 					enabled="false" />
		<object name="g_spotlight_shaft" 			enabled="false" />
		<object name="g_light_cone" 				enabled="false" />
		<object name="g_blue_halo" 					enabled="false"  />
		<object name="g_red_halo_down" 				enabled="false"  />
		<object name="g_red_halo_right" 			enabled="false"  />
		<object name="g_red_halo_up" 				enabled="false"  />
		<object name="g_light_source_white" 		enabled="false"  />
		<object name="g_light_source_red_blink" 	enabled="false"  />
		<object name="g_light_source_red" 			enabled="false"  />
		<object name="g_light_source_blue" 			enabled="false"  />
	</graphic_group>

	<graphic_group name="grp_money_pile" enabled="true" culling_object="co_helicopeter">
			<lod_object name="lod_money">
				<object name="g_money_pile_lod0" 		enabled="true"  max_distance="3000"  max_draw_lod="0" />
				<object name="g_money_pile_lod1" 		enabled="true" lod="1" />
			</lod_object>
			<lod_object name="lod_hook" enabled="true"	culling_object="co_helicopeter">
				<object name="g_hook_lod0" 		enabled="true"  max_distance="3000"  max_draw_lod="0" />
				<object name="g_hook_lod1" 		enabled="true" lod="1" />
			</lod_object>
			<lod_object name="lod_net" enabled="true" 	culling_object="co_helicopeter">
				<object name="g_net_lod0" 		enabled="true"  max_distance="3000"  max_draw_lod="0" />
				<object name="g_net_lod1" 		enabled="true" lod="1" />
			</lod_object>

				<object name="g_wire"				enabled="true" shadow_caster="false" />

				<object name="g_strap1"				enabled="true" shadow_caster="false" />
				<object name="g_strap2"				enabled="true" shadow_caster="false" />
				<object name="g_strap3"				enabled="true" shadow_caster="false" />
				<object name="g_strap4"				enabled="true" shadow_caster="false" />

				<object name="s_strap1"				enabled="false" shadow_caster="true" />
				<object name="s_strap2"				enabled="false" shadow_caster="true" />
				<object name="s_strap3"				enabled="false" shadow_caster="true" />
				<object name="s_strap4"				enabled="false" shadow_caster="true" />

				<object name="s_money_pile"			enabled="false" shadow_caster="true" />
				<object name="s_stand"				enabled="false" shadow_caster="true" />
				<object name="s_wire"				enabled="false" shadow_caster="true" />
				<object name="s_hook"				enabled="false" shadow_caster="true" />
	</graphic_group>
  </graphics>
  <decal_surfaces default_material="metal">           
		<decal_mesh name="dm_door_left_nypd" 	enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_door_right_nypd" 	enabled="true" material="sheet_metal" />
		<decal_mesh name="dm_body" 				enabled="true" material="metal_chassis" />
		<decal_mesh name="g_glass" 				enabled="true" material="glass_breakable" />
	</decal_surfaces>
	<effects>
		<effect_spawner name="flashlight" object="align_spotlight_effect" effect="effects/particles/weapons/flashlight/flashlight" enabled="false" assign_parent="true"/>
	</effects>
	<lights>
		<light multiplier="identity" far_range="4000" name="ls_spotlight" enabled="false" type="spot|specular" />
	</lights>
</dynamic_object>