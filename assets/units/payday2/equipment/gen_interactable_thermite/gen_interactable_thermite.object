			<object name="g_thermite_melted_block_2" 	enabled="false"/>
			<object name="g_thermite_melted_block_3" 	enabled="false"/>
			<object name="g_thermite_melted_block_4" 	enabled="false"/>
			<object name="g_thermite_melted_block_5" 	enabled="false"/>
			<object name="g_thermite_melted_block_6" 	enabled="false"/>
