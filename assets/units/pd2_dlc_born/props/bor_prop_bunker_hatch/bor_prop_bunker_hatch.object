<dynamic_object>
	<diesel materials="units/pd2_dlc_born/props/bor_prop_bunker_hatch/bor_prop_bunker_hatch" orientation_object="rp_bor_prop_bunker_hatch" />
	<sequence_manager file="units/pd2_dlc_born/props/bor_prop_bunker_hatch/bor_prop_bunker_hatch" />
	<animations>
		<animation_group name="anim" enabled="false">
			<object name="anim_hatch" />
			<object name="anim_wheel" />
		</animation_group>
	</animations>
	<bodies>
		<body name="body_static" enabled="true" template="animated">
			<object name="anim_hatch" />
			<object name="c_box_01" collision_type="box" padding="-2.5" />
			<object name="c_box_02" collision_type="box" padding="-2.5" />
		</body>
	</bodies>
	<decal_surfaces default_material="metal">
		<decal_mesh name="dm_metal" enabled="true" material="metal" />
	</decal_surfaces>
	<graphics>

		<lod_object name="lod_g">
	        <object enabled="true" name="g_g" >
			  	<material materials="mat_hatch" max_distance="5000"/>
	        	<material materials="mat_hatch_lod" />
			</object>
		</lod_object>		

		<object name="g_g" enabled="true" shadow_caster="true"/>
		<object name="g_wheel" enabled="true" shadow_caster="true"/>
		<object name="s_s" enabled="true" shadow_caster="true" />
	</graphics>
</dynamic_object>
